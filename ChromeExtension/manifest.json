{
    "manifest_version":2,
    "name": "Shreyas Chrome Extension",
    "version": "0.001",
    "web_accessible_resources": [
        "*.png"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    
    "browser_action":{
        "default_icon":"icon.png"
    }
}