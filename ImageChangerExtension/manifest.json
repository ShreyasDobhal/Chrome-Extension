{
    "manifest_version":2,
    "name": "Image Changer Extension",
    "version": "0.001",
    "web_accessible_resources": [
        "images/*.png"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    
    "browser_action":{
        "default_icon":"icon.png"
    }
}